<script>
$(document).reday( function(){
alert("running script");

$("#btnOneId").click(function() {
	alert("clecked");
    var inputId = document.getElementById("t_ticket").value;
    vae theUrl="Name of the url to get ticket by id"+ inputId;
    $.ajax( {
    	url: theUrl
        type:"GET",
        dataType:"text",
        success: function (result){
        alert("succes");
        $("#p_getTicket").html(result); },
        error:function(xhr){
        alert("error");
        $("#p_getTicket").html("Error:" + xhr.status+" "+xhr.statusText);}
        });
        });
